<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<source_library title="source library">
  <source name="Fermi bubbles" type="DiffuseSource">
    <spatialModel type="DiffuseMap" file="fermibubbles_map.fits">
      <parameter name="Prefactor" scale="1" value="1" min="0.001" max="1000.0" free="0"/>
    </spatialModel>
    <!-- Spectral shape from Herold and Malyshev DOI:
    10.1051/0004-6361/201834670, cutoff was set at 10 TeV = 10x 95%
    lower limit on cutoff energy from Fermi obs
    -->
    <spectrum type="ExponentialCutoffPowerLaw">
  <parameter name="Prefactor" scale="1e-10" value="5"    min="1e-07" max="1000.0" free="1"/>
  <parameter name="Index"        scale="-1"    value="2.05" min="0.0"   max="+5.0"   free="1"/>
  <parameter name="CutoffEnergy" scale="1e6"   value="10.0"  min="0.01"  max="1000.0" free="1"/>
  <parameter name="PivotEnergy"    scale="1e3"   value="1.0"  min="0.01"  max="1000.0" free="0"/>
    </spectrum>
    </source>
</source_library>
